function removeEntryByProperty<T>(array: T[], property: keyof T, value: any): void {
  for (let i = 0; i < array.length; i++) {
    if (array[i][property] === value) {
      array.splice(i, 1);
      break; // Exit loop after removing the first matching entry
    }
  }
}

