public mapUserPreferencesToDto(
  quickViews: QuickViewEntry[],
  currentUser: IO_CurrentUser,
  type: string // 'SOR' or 'WeeklySummary'
): UserPreferences_dto {
  let dto: UserPreferences_dto = {
    gems: currentUser.gems,
    id: currentUser.gems,
    sorColumnManagement: [],
    weeklySummaryColumnManagement: [],
    lastUpdatedBy: currentUser.name,
    lastUpdated: new Date().toISOString(),
  };

  if (type === 'SOR') {
    dto.sorColumnManagement = this.mapSorQuickViewsToDto(
      quickViews as SORQuickViewEntry[]
    );
  } else if (type === 'WeeklySummary') {
    dto.weeklySummaryColumnManagement = this.mapWeeklySummaryQuickViewsToDto(
      quickViews as WeeklySummaryQuickViewEntry[]
    );
  } else {
    throw new Error('Unsupported type: ' + type);
  }

  return dto;
}