if (type === 'SOR') {
    const newValues = this.mapSorQuickViewsToDto(_up.sorQuickViews || []);
    const uniqueValues = newValues.filter(
        (item) => !dto.sorColumnManagement.some((existing) => JSON.stringify(existing) === JSON.stringify(item))
    );
    dto.sorColumnManagement.push(...uniqueValues);
} else if (type === 'WeeklySummary') {
    const newValues = this.mapWeeklySummaryQuickViewsToDto(_up.weeklySummaryQuickViews || []);
    const uniqueValues = newValues.filter(
        (item) => !dto.weeklySummaryColumnManagement.some((existing) => JSON.stringify(existing) === JSON.stringify(item))
    );
    dto.weeklySummaryColumnManagement.push(...uniqueValues);
} else {
    throw new Error('Unsupported type: ' + type);
}