let dto: UserPreferences_dto = {
    id: currentUser.gems,
    sorColumnManagement: Array.from(new Set([
        ...dto.sorColumnManagement,
        ...this.mapSorQuickViewsToDto(_up.sorQuickViews || [])
    ])),
    weeklySummaryColumnManagement: Array.from(new Set([
        ...dto.weeklySummaryColumnManagement,
        ...this.mapWeeklySummaryQuickViewsToDto(_up.weeklySummaryQuickViews || [])
    ])),
    lastUpdatedBy: currentUser.name,
    lastUpdated: new Date().toISOString(),
};